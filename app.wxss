/**app.wxss**/
@import "/wxParse/wxParse.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.flex_warp{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.flex_nowarp{
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
h1,h2,h3,h4,h5,h6,p,ul,ol{
  font-size: 100%;
  margin: 0;
  padding: 0;
}
ul,ol{
  list-style: none;
}
.left{
  float: left;

}
.right{
  float: right;
}
.clearfix{
  clear: both;display: block;
  content: "";
}
.text-center{
  text-align: center;
}
.text-left{
  text-align: left;
}
.text-right{
  text-align: right;
}
.wxParse{
  margin: 0 18rpx;
}
page{
  background: #f2f2f2;
}